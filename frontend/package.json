    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io-client": "^3.0.0",
    "@types/three": "^0.180.0",
    "axios": "^1.11.0",
    "d3": "^7.8.5",
    "formik": "^2.4.6",
    "jspdf": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-intl": "^7.1.11",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2",
